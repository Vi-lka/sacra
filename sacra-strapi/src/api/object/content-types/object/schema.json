{
  "kind": "collectionType",
  "collectionName": "objects",
  "info": {
    "singularName": "object",
    "pluralName": "objects",
    "displayName": "Объект",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "region": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "component": "objects.region"
    },
    "location": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "geolocation": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "component": "objects.geolocation"
    },
    "dateOfConstruction": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "objects.date-construction",
        "objects.date-construction-list"
      ],
      "required": false,
      "max": 1,
      "min": 1
    },
    "appearanceChanges": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "objects.appearance-changes",
        "objects.appearance-changes-date-list"
      ]
    },
    "historicalNote": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": false,
      "maxLength": 5000
    },
    "imagesSlider": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "videos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "urlTour": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "confession": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "objects.confession",
      "required": true
    },
    "order": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger",
      "required": false
    },
    "architect": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::architect.architect"
    },
    "architecturalStyles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::architectural-style.architectural-style"
    },
    "sources": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "objects.sources"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "model3d": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::models3d.models3d"
    }
  }
}
