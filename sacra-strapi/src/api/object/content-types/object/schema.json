{
  "kind": "collectionType",
  "collectionName": "objects",
  "info": {
    "singularName": "object",
    "pluralName": "objects",
    "displayName": "Объект",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "location": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false
    },
    "geolocation": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "component": "objects.geolocation"
    },
    "historicalNote": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext",
      "required": false,
      "maxLength": 5000
    },
    "imagesSlider": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "videos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "urlTour": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "order": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "required": false,
      "unique": true
    },
    "architects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::architect.architect",
      "inversedBy": "objects"
    },
    "architectural_styles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::architectural-style.architectural-style",
      "inversedBy": "objects"
    },
    "sources": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "objects.sources"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "models": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::models3d.models3d",
      "mappedBy": "object"
    },
    "dateConstruction": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "appearanceChangesList": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "objects.appearance-changes"
    },
    "confession": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::confession.confession",
      "inversedBy": "objects"
    },
    "region": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::region.region",
      "inversedBy": "objects"
    },
    "district": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::district.district",
      "inversedBy": "objects"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "objects"
    },
    "architectsString": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "tour": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tour.tour",
      "mappedBy": "object"
    }
  }
}
